<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NoteView">
        <attr name="fontSize" format="dimension"></attr>
        <attr name="lightColor" format="color"></attr>
        <attr name="warnColor" format="color"></attr>
        <attr name="normalColor" format="color"></attr>
    </declare-styleable>
    <declare-styleable name="NumberPassWordEditView">
        <attr name="length" format="integer" />
        <attr name="lineWidth" format="dimension" />
        <attr name="lineColor" format="color" />
        <attr name="textAppearance" format="reference" />
    </declare-styleable>
    <declare-styleable name="BaseNestScrollView">
        <attr name="layout_head" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CardImageView">
        <attr name="cardBackgroundColor" format="color" />
        <attr name="cardCornerRadius" format="dimension" />
    </declare-styleable>
    <declare-styleable name="RoundedImageView">
        <attr name="cardRadius" format="dimension" />
        <attr name="leftTopRadius" format="dimension" />
        <attr name="rightTopRadius" format="dimension" />
        <attr name="leftBottomRadius" format="dimension" />
        <attr name="rightBottomRadius" format="dimension" />
    </declare-styleable>
</resources>